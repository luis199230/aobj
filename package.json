{
  "name": "ts-obj",
  "version": "1.0.6",
  "description": "ts-object - Object Manipulation Module for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "jest"
  },
  "keywords": [
    "object",
    "utilities",
    "manipulation",
    "deep",
    "map",
    "clone",
    "isObject",
    "isEmpty",
    "traverse",
    "filter",
    "invert"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luis199230/ts-aobj/issues"
  },
  "homepage": "https://github.com/luis199230/ts-aobj#readme",
  "dependencies": {
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
